input AddressInput {
    addressLine: String!
    country: String!
    city: String!
    postalCode: String!
}

input TransactionProductInput {
    productId: Int!
    productQuantity: Int!
}

extend type Mutation {
    buyProducts(products: [TransactionProductInput!]!, address: AddressInput!): Boolean
}
