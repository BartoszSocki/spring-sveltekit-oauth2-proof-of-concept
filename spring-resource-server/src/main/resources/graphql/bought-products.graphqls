type BoughtProduct {
    id: ID!
    name: String!
    price: Money!
    ownerId: Int!
    category: String!
    date: String!

    description: String
    imageUrl: String
}

type BoughtProductPage {
    content: [BoughtProduct]!
    isFirstPage: Boolean!
    isLastPage: Boolean!
}

extend type Query {
    searchBoughtProducts(pageable: PageableInput!, sort: SortInput!): BoughtProductPage!
}